{
	"name": "externaldata",
	"version": "1.0.0",
	"description": "An example of using external data sources to drive the creation of a property",
	"main": "index.js",
	"dependencies": {
		"enum": "^3.0.4",
		"akj-tech-preview": "^1.3.3"
	},
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-jest": "^28.2.0",
		"eslint-plugin-jsdoc": "^48.0.4",
		"eslint-plugin-prettier": "^5.1.3",
		"jest": "^29.7.0",
		"prettier": "^3.2.5",
		"prettier-plugin-jsdoc": "^1.3.0"
	},
	"scripts": {
		"clean": "rimraf ./out/ ./node_modules/ ./package-lock.json",
		"format": "prettier --config .prettierrc '{src,tests}/**/*.{js,json}' --write",
		"lint": "eslint . --ext .js --resolve-plugins-relative-to ./",
		"lint-fix": "eslint . --ext .js --fix --resolve-plugins-relative-to ./",
		"test": "NODE_OPTIONS=--experimental-vm-modules npx jest --unhandled-rejections=strict"
	},
	"license": "ISC",
	"eslintConfig": {
		"env": {
			"node": true
		},
		"plugins": [
			"prettier",
			"jsdoc",
			"jest"
		],
		"extends": [
			"eslint:recommended",
			"plugin:prettier/recommended",
			"plugin:jsdoc/recommended",
			"plugin:jest/recommended"
		],
		"rules": {
			"no-console": 1,
			"prettier/prettier": "warn",
			"jsdoc/tag-lines": [
				"error",
				"never",
				{
					"startLines": null
				}
			]
		}
	}
}
